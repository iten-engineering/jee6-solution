<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
	<description>Hello EJB2</description>
	<display-name>Hello EJB2</display-name>
	<enterprise-beans>
		<session>
			<ejb-name>HelloEJB2Bean</ejb-name>
			<home>ch.itenengineering.helloejb2.ejb.HelloEJB2Home</home>
			<remote>ch.itenengineering.helloejb2.ejb.HelloEJB2Remote</remote>
			<ejb-class>ch.itenengineering.helloejb2.ejb.HelloEJB2Bean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>
</ejb-jar>
 
